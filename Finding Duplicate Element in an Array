#include <iostream>
using namespace std;
int main(){
    int n;
    cout<<"Enter the size of array \n";
     cin>>n;
     int arr[n];
     cout<<"Enter the elements of array \n";
    for(int i=0;i<n;i++){
         cin>>arr[i];
         }
    int ans =0;
    for(int i=0;i<n;i++){
    ans^=arr[i];
    }
    for(int i=0;i<n;i++){
        ans^=i;
    }
    cout<<"The duplicate element is :"<< ans;
    return 0;
}
